{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "gulp serve",
    "start:webpack": "webpack-dev-server --open --mode development",
    "start:rollup": "rm -rf dist && rollup -c --watch",
    "build:rollup": "rm -rf dist && rollup -c rollup.config.prod.js",
    "build": "tsc && polymer build --auto-base-path && gulp prpl-server",
    "build:static": "tsc && polymer build",
    "build:webpack": "rm -rf dist && webpack --config webpack.config.js",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:webpack": "polymer serve --port 3000 dist",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/polymer": "^3.1.0",
    "@types/ramda": "^0.26.26",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "core-js": "^3.2.1",
    "jexcel": "^3.5.0",
    "jsuites": "^2.1.3",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.0",
    "ramda": "^0.26.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "ts-loader": "^6.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.6.0",
    "@polymer/test-fixture": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "@vaadin/vaadin-combo-box": "^5.0.6",
    "@vaadin/vaadin-grid": "^5.4.9",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^5.0.4",
    "del": "^5.1.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.18.2",
    "prpl-server": "^1.4.0",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-serve": "^1.0.2",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-uglify": "^6.0.3",
    "typescript": "^3.1.3",
    "webpack": "4.41.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
